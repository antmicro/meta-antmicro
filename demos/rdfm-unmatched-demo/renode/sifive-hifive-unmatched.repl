using "./sifive-fu740.repl"

tmp451: Mocks.DummyI2CSlave @ i2c1 0x4c

virtio: Storage.VirtIOBlockDevice @ sysbus 0x100d0000
    IRQ -> plic@54

flipflop: Python.PythonPeripheral @ {
    sysbus 0x100d0898;
    sysbus 0x100d08d8
}
    size: 0x8
    initable: true
    filename: "scripts/pydev/flipflop.py"
